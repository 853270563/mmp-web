<?xml version="1.0" encoding="utf-8"?>
<trans name="custExecutives" desc="客户高管列表">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000100014" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
  				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field name="customer_id" client="customerId" type="C"  required="true"  default=""    desc="客户编号" /> 
				<field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
					<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="分页标非零0分页!0不分页" />
					<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
					<field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="10"    desc="分页大小" />
			</field-extend>	 
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
	<head>
		<field-response name="service_response"  xpath="Service_Header/service_response" type="S" desc="解析响应报文头">
			<field name="status"     required="true" length="19"     type="C" default="" desc=""/>
			<field name="code"     required="true" length="19"     type="C" default="" desc=""/>
			<field name="desc"     required="true" length="19"     type="C" default="" desc=""/>
		</field-response>
	</head>
			<field name="total"     required="true" length="10"   xpath="Service_Body/response/total"   type="C" default="01" desc="总数"/>
			<!-- desc 格式说明    业务品种，字段含义/业务品种，字段含义-->
   			<field name="item"  xpath="Service_Body/response/item"  type="E"  desc="解析响应报文体">
					<field name="relativeID" client="relativeID" type="C" required="true"  default="20"    desc="高管客户ID" />  
					<field name="customerName" client="customerName" type="C" required="true"  default="20"    desc="高管姓名" />  
					<field name="certType" client="certType" type="C" required="true"  default="20"    desc="客户证件类型" />  
					<field name="certID" client="certID" type="C" required="true"  default="20"    desc="证件号码" />  
					<field name="customerType" client="customerType" type="C" required="true"  default="20"    desc="客户类型" />  
					<field name="countryCodeName" client="countryCodeName" type="C"  default="20"    desc="国籍" />  
					<field name="sex" client="sex" type="C" required="true"  default="20"    desc="性别" />  
					<field name="birthday" client="birthday" type="C" required="true"  default="20"    desc="出生日期" />  
					<field name="duty" client="duty" type="C" required="true"  default="20"    desc="学历" />  
					<field name="passportDate" client="passportDate" type="C" required="true"  default="20"    desc="证件起始日期" />  
					<field name="relationShip" client="relationShip" type="C" required="true"  default="20"    desc="担任职务" />  
					<field name="oughtSum" client="oughtSum" type="C" required="true"  default="20"    desc="应出资金额" />  
					<field name="passportMaturity" client="passportMaturity" type="C" required="true"  default="20"    desc="证件到期日期" />  
					<field name="describe" client="describe" type="C" required="true"  default="20"    desc="工作简历" />  
					<field name="telephone" client="telephone" type="C" required="true"  default="20"    desc="联系电话" />
					<field name="engageTerm" client="engageTerm" type="C" required="true"  default="20"    desc="相关专业从业年限" />  
					<field name="holdStock" client="holdStock" type="C" required="true"  default="20"    desc="持股情况（限100个汉字）" />    
					<field name="inputUserName" client="inputUserName" type="C"   default="20"    desc="登记人" />  
					<field name="inputOrgName" client="inputOrgName" type="C" required="true"  default="20"    desc="登记机构" />  
					<field name="inputDate" client="inputDate" type="C" required="true"  default="20"    desc="登记日期" /> 
					<field name="updateDate" client="updateDate" type="C" required="true"  default="20"    desc="更新日期" />   
			</field>
		</rcv>
</trans>
