<?xml version="1.0" encoding="utf-8"?>
<trans name="netloan/SP0065" excode="Contract_APP03" desc="APP总量授信合同查询">
	<snd>
		<head>
			<field name="tranType" type="C" desc="交易类型" required="true" default="0"/>
			<field name="fileName" type="C" desc="文件名称" required="false"/>
		</head>
		<body>
			<field name="manager_code" length="32" type="C" desc="客户经理员工号" required="true" />						
			<field name="bar_code" length="32" type="C" desc="条码号" required="true"/>          
		</body>	
	</snd>	
	<rcv>
		<head>
			<field name="txnCod" type="C" desc="交易代码"/>
			<field name="tranType" type="C" desc="交易类型"/>
			<field name="fileName" type="C" desc="文件名称"/>
			<field name="termDate" type="C" desc="交易日期"/>
			<field name="termTime" type="C" desc="交易时间"/>
		</head>
		<body>
			<field name="result" length="50" type="C" desc="结果"/>               
			<field name="manager_code" length="32" type="C" desc="客户经理员工号" />               
			<field name="bar_code" length="32" type="C" desc="条码号"/>  
			<field name="reason_fail" length="100" type="C" desc="失败原因"/>    
			<field name="totalAmount" type="M" desc="总量授信"> 
				<field name="loan_money" length="20" type="C" desc="总量授信额度"/>    
				<field name="rest_total_money" length="20" type="C" desc="总量授信余额"/>    
				<field name="start_date" length="20" type="C" desc="起始日期"/>    
				<field name="end_date" length="20" type="C" desc="到期日期"/>    
				<field name="period" length="20" type="C" desc="期限"/>    
				<field name="total_amount_contract_no" length="100" type="C" desc="个人综合授信合同编号"/>
			</field>  
		</body>
	</rcv>
</trans>
