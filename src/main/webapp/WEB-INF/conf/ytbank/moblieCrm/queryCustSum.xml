<?xml version="1.0" encoding="utf-8"?>
<trans name="queryCustSum" ibatisStatement="" desc="客户总数（支行下客户经理）">
    <!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
    <snd>
        <!-- 客户信息  -->
        <field  name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"  />
        <!-- <field  name="pageIndex" length="30" type="C" desc="第几页"  />
        <field  name="pageSize" length="30" type="C" desc="每一页大小"  />  -->
        <field name="orgID" length="20" type="C" desc="机构id（03 公司开业 05客户生日）" required="false"/>


    </snd>
    <!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
    <rcv>
        <!--公共字段 -->
        <field name="STATUS" length="6" type="C" desc="返回码" />
        <field name="MSG" length="100" type="C" desc="返回消息" />
        <field name="SEQ" length="16" type="C" desc="流水号" />
        <field name="TRANS_TYPE" length="4" type="C" desc="交易类型" /><!-- 0001：新增客户进件信息 -->
        <field name="TRANS_CODE" length="4" type="C" desc="交易结果码" /><!-- 0000：表示交易成功 其它均为交易失败 -->
        <field name="TRANS_DESC" length="128" type="C" desc="交易结果描述" />
        <field name="TRANS_DATE" length="8" type="C" desc="响应日期" />
        <field name="TRANS_TIME" length="6" type="C" desc="响应时间" />

        <field-list name="item" type="E">
            <field name="custName " length="20" type="C" desc="客户姓名 " required="true"/>
            <field name="kpiValue" length="20" type="C" desc="指标当前值" required="true"/>
            <field name="kpiValueld" length="20" type="N" desc="比上日（计算）" required="true"/>
            <field name="kpiValuelm" length="20" type="N" desc="比上月（计算）"/>
            <field name="kpiValuely" length="20" type="N" desc="比年初（计算）"/>
        </field-list>
    </rcv>
</trans>