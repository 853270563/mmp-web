<?xml version="1.0" encoding="utf-8"?>
<trans name="CheckBussiCardStatus" ibatisStatement="" desc="查询银行卡状态">
	<!-- 请求报文 -->
	<snd> 
		<field name="CUST_ID" length="20" type="C" desc="客户号" />
		<field name="BUSSICARDID" length="20" type="C" desc="卡号" />
	</snd>
	<!-- 响应报文 -->
	<rcv> 
		<!--公共字段-->
		<field name="STATUS" length="6" type="C" desc="返回码" /> 
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field name="STATE" length="20" type="C" desc=" 持卡状态1//正常状态" /> 
	</rcv>
</trans>
