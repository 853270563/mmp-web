<?xml version="1.0" encoding="utf-8"?>
<trans name="currentOpitions" ibatisStatement="" desc="审批人意见选择">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 客户信息  -->
		<field  name="PAGE_NO" length="30" type="C" desc="第几页"  />
		<field  name="PAGE_SIZE" length="30" type="C" desc="每一页大小"  />  
		<field  name="serialNo" length="10" type="C" desc="流水号 "/>
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		 <!-- 代办记录 -->
        <field-list name="LIST" type="E">
           <field name="serialNo" client="serialNo" type="C" required="true"  default="20"    desc="流程编号" />  
			<field name="phaseOpinion1" client="phaseOpinion1" type="C" required="true"  />  
        </field-list>
	</rcv>
</trans>