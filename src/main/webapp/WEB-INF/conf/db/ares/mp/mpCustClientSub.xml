<?xml version="1.0" encoding="utf-8"?>
<trans name="mpCustClientSub" desc="推送订阅">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd> 
		<field name="CLIENT_TYPE" length="1" type="C" desc="终端类型 0-IOS 1-安卓"  required="true"/>
		<field name="DEVICE_ID" length="64" type="C" desc="设备标识"  required="true"/>
		<field name="CLIENT_APP_ID" length="64" type="C" desc="客户端应用编号"  required="true"/>
		<field name="CUST_ID" length="20" type="C" desc="签约客户号"  required="true"/>
		<field name="DEVICE_NAME" length="256" type="C" desc="设备名称"  required="false"/>
		<field name="DEVICE_MODEL" length="128" type="C" desc="设备型号"  required="false"/>
		<field name="DEVICE_VERSION" length="64" type="C" desc="设备版本"  required="false"/>
		<field name="IS_CLOSE" length="1" type="C" desc="是否关闭0-否 1-是"  required="false"/>
		<field name="CHAN_TYPE" length="1" type="C" desc="通道类型"  required="true"/>
	</snd>
	
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<field name="CUST_CLIENT_ID" length="16" type="C" desc="主键" />
        <field name="CLIENT_ID" length="128" type="C" desc="消息主题标示"/>
	</rcv>
</trans>
