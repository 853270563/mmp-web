<?xml version="1.0" encoding="utf-8"?>
<trans name="00080000200003"  desc="预警事项查询" >
	<!--请求报文 -->
	<snd>
				<field  name="CustomerName" length="80" type="C" desc="客户姓名 "/>
				<field  name="CertID" length="40" type="C" desc="证件号码" />
				<field  name="SignalType" length="40" type="C" desc="预警类型"   mapKey="01 一半预警02 重大预警"/>
				<field  name="BelongManDepart" length="40" type="C" desc="归属管理部门"   mapKey="0310 个人业务部0110 公司业务部0120 小企业贷款部"/>
				<!--分页-->
				<field name="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" /> 
				<field name="PAGE_SIZE"  type="C" required="true"  default="10"    desc="分页大小" />
	</snd>
	<!-- 响应报文 -->
		<rcv>
				<field name="STATUS" length="6" type="C" desc="返回码" />
				<field name="MSG" length="100" type="C" desc="返回消息" />
				<field name="TOTAL_NUM"  type="C"  desc="总条数" />
				<field name="COUNT"  type="C"  desc="条数" />
   			    <field-list name="LIST"   type="E"  desc="解析响应报文体">
					<field name="CustomerName" length="16" type="C" desc="客户姓名" />
					<field name="SerialNo" length="4" type="C" desc="预警信息流水号" />
					<field name="CertName" length="4" type="C" desc="证件名称" />
					<field name="CertID" length="32" type="C" desc="证件号码" />
					<field name="MessageContent" length="8" type="C" desc="预警信号" />
					<field name="SignalType" length="20" type="C" desc="归属类型" />
					<field name="BelongManDepart" length="20" type="C" desc="归属管理部门" />
					<field name="InputUserName" length="20" type="C" desc="登记人" />
					<field name="InputOrgName" length="20" type="C" desc="登记人归属机构" />
					<field name="InputDate" length="20" type="C" desc="登记时间" />
					<field name="UpdateDate" length="20" type="C" desc="更新时间" />
				</field-list>
		
		</rcv>
</trans>
