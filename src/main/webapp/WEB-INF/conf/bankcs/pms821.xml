<?xml version="1.0" encoding="utf-8"?>
<trans name="TranDeGetPin" desc="获取凭证号接口">
	<snd>
		<field name="bzh" length="2" type="C" desc="币种号" required="true"/>
		<field name="czy" length="4" type="C" desc="操作员" required="true"/>
		<field name="jgm" length="9" type="C" desc="机构码" required="true"/>
		<field name="sblx" length="1" type="C" desc="设备类型" required="true"/>
		<field name="jym" length="4" type="C" desc="交易码" required="true"/>
		<field name="sqczy" length="4" type="C" desc="授权操作员" />
		<field name="sqczymm" length="16" type="C" desc="授权操作员密码" />
		<field name="pzzl" length="8" type="C" desc="凭证种类" />
	</snd>
	<rcv>
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="70" type="C" desc="返回消息" />
		<field name="zjlsh" length="8" type="C" desc="主机流水号" />
		<field name="czylsh" length="8" type="C" desc="操作员流水号" />
		<field name="pzhm" length="20" type="C" desc="凭证号码" />
	</rcv>
</trans>
