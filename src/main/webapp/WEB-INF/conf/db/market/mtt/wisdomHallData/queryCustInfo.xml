<?xml version="1.0" encoding="utf-8"?>
<trans name="queryCustInfo" desc="客户信息" ibatisStatement="">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<field name="CUST_NO" length="20" type="C" desc="客户号" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field name="CUST_NO" length="20" type="C" desc="客户号" />
		<field name="TICKET_TIME" length="20" type="C" desc="取号时间yyyy-MM-dd HH:mm:ss" />
		<field name="WAIT_TIME" length="20" type="C" desc="等待时间" />
		<field name="CUST_LEVEL" length="2" type="C" desc="客户登记0:普通;1:一星级;2：…" />
		<!-- 客户信息列表 -->
		<field name="CUST_NAME" length="50" type="C" desc="客户名称" />
		<field name="SEX" length="1" type="C" desc="性别0:男;1:女;2:未知" />
		<field name="BIRTHDAY" length="20" type="C" desc="yyyy-MM-dd" />
		<field name="MARITAL_STATUS" length="1" type="C" desc="婚姻状况0:未婚;1:已婚;2:离异;3:丧偶 " />
		<field name="NATIONALITY" length="20" type="C" desc="国籍" />
		<field name="CAREER" length="20" type="C" desc="职业" />
		<!-- 兴趣爱好 -->
		<field-list name="HOBBY_LIST" type="E">
			<field name="HOBBY_NAME" length="20" type="C" desc="兴趣名称" />
		</field-list>
		<!-- 持有产品 -->
		<field-list name="HOLD_PROD_LIST" type="E">
			<field name="PROD_NO" length="20" type="C" desc="产品编号" />
			<field name="PROD_NAME" length="20" type="C" desc="产品名称" />
		</field-list>
		
	</rcv>
</trans>
