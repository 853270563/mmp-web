<?xml version="1.0" encoding="utf-8"?>
<trans name="groupCreditDisInfo" ibatisStatement="" desc="关联额度信息">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 关联额度信息  -->
		<field name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"/>
		<field name="apply_no" client="applySerialNo" type="C"  required="true"  default=""    desc="业务申请编号" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码"/>
		<field name="MSG" length="100" type="C" desc="返回消息"/>
		<field name="SEQ" length="16" type="C" desc="流水号"/>
		<field name="TRANS_TYPE" length="4" type="C" desc="交易类型"/><!-- 0001：新增客户进件信息 -->
		<field name="TRANS_CODE" length="4" type="C" desc="交易结果码"/><!-- 0000：表示交易成功 其它均为交易失败 -->
		<field name="TRANS_DESC" length="128" type="C" desc="交易结果描述"/>
		<field name="TRANS_DATE" length="8" type="C" desc="响应日期"/>
		<field name="TRANS_TIME" length="6" type="C" desc="响应时间"/>
		<!-- 代办记录 -->
		<field-list name="LIST" type="E">
			<field name="RelativeSerialNo" client="RelativeSerialNo" type="C" required="true"  default="100"    desc="额度协议号" />
			<field name="CustomerName" client="CustomerName" type="C" required="true"  default="100"    desc="客户名称" />
			<field name="CertType" client="CertType" type="C" required="true"  default="100"    desc="证件类型" />
			<field name="CertID" client="CertID" type="C" required="true"  default="100"    desc="证件号码" />
			<field name="BusinessType" client="BusinessType" type="C" required="true"  default="100"    desc="业务品种代码" />
			<field name="BusinessTypeName" client="BusinessTypeName" type="C" required="true"  default="100"    desc="额度名称" />
			<field name="BusinessSum" client="BusinessSum" type="C" required="true"  default="100"    desc="额度金额" />
			<field name="PutOutDate" client="PutOutDate" type="C" required="true"  default="100"    desc="额度起始日" />
			<field name="Maturity" client="Maturity" type="C" required="true"  default="100"    desc="额度到期日" />
		</field-list>
	</rcv>
</trans>