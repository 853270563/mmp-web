<?xml version="1.0" encoding="utf-8"?>
<trans name="queryReferralInfo" desc="客户信息" ibatisStatement="">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<field name="SENDER" length="20" type="C" desc="转介人" />
		<field name="RECEIVER" length="20" type="C" desc="接收人" />
		<field name="REFERRAL_STATUS" length="1" type="C" desc="转介状态0:待处理,1:已接受,2:已拒绝,3:已撤回" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析 -->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field-list name="REFERRAL_LIST" type="E">
			<field name="REFERRAL_ID" length="20" type="C" desc="转介编号" />
			<field name="SENDER_ID" length="20" type="C" desc="转介者" />
			<field name="SENDER_NAME" length="50" type="C" desc="转介者名称" />
			<field name="RECEIVER_ID" length="20" type="C" desc="接收者" />
			<field name="RECEIVER_NAME" length="50" type="C" desc="接收者名称" />
			<field name="REFERRAL_STATUS" length="20" type="C" desc="转介状态0:待处理,1:已接受,2:已拒绝,3:已撤回" />
			<field name="REFERRAL_TIME" length="20" type="C" desc="转介时间" />
			<field name="PROD_NAME" length="50" type="C" desc="产品名称" />
			<field name="CUST_NAME" length="50" type="C" desc="客户名称" />
			<field name="MOBILE_NO" length="20" type="C" desc="手机号码" />
		</field-list>
	</rcv>
</trans>
