<?xml version="1.0" encoding="utf-8"?>
<trans name="queryYXSJ" desc="营销商机（客户个人资料下）" serviceId="00850000000056">
    <!--请求报文 -->
    <snd>

        <field name="custId" length="20" type="C" desc="客户id(03 公司开业 05客户生日)" required="false"/>
        <field name="status" length="20" type="C" desc="状态(0 未处理 1 逾期未处理 2 已完成 3 逾期处理)" required="false"/>
        <field name="userId" length="100" type="C" desc="登录号" required="true"/>
    </snd>
    <!-- 响应报文 -->
    <rcv>
        <field name="item"  xpath="/Service/Service_Body/response/item" client="LIST" type="E"  desc="解析响应报文体">
            <field name="mbrePurpRemark " length="120" type="C" desc="商机意向 " required="true"/>
            <field name="mbrePurp" length="20" type="C" desc="产品名称（商机类型）" required="true"/>
            <field name="mbreStatus" length="20" type="C" desc="商机状态" required="true"/>
        </field>
    </rcv>
</trans>