<?xml version="1.0" encoding="utf-8"?>
<trans name="showSign" ibatisStatement="" desc="是否显示签署意见">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 客户信息  -->
		 <field  name="flowNo"  length="30"  type="C"  desc="阶段编号"/>
		 <field  name="phaseNo"  length="30"  type="C"  desc="意见名称"/>
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		 <!-- 代办记录 -->
        <field-list name="LIST" type="E">
           <field name="displaySignOpinion" client="displaySignOpinion" type="C" required="true"  default="20"   />  
        </field-list>
	</rcv>
</trans>
	