<?xml version="1.0" encoding="utf-8"?>
<trans name="peopleEducation" ibatisStatement="" desc="个人学业履历">
    <!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
    <snd>
        <!-- 个人学业履历  -->
        <field name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"/>
        <field name="customer_id" client="customerId" type="C" required="true" default="" desc="客户编号"/>
    </snd>
    <!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
    <rcv>
        <!--公共字段 -->
        <field name="STATUS" length="6" type="C" desc="返回码"/>
        <field name="MSG" length="100" type="C" desc="返回消息"/>
        <field name="SEQ" length="16" type="C" desc="流水号"/>
        <field name="TRANS_TYPE" length="4" type="C" desc="交易类型"/><!-- 0001：新增客户进件信息 -->
        <field name="TRANS_CODE" length="4" type="C" desc="交易结果码"/><!-- 0000：表示交易成功 其它均为交易失败 -->
        <field name="TRANS_DESC" length="128" type="C" desc="交易结果描述"/>
        <field name="TRANS_DATE" length="8" type="C" desc="响应日期"/>
        <field name="TRANS_TIME" length="6" type="C" desc="响应时间"/>
        <!-- 代办记录 -->
        <field-list name="LIST" type="E">
            <field name="CustomerID" client="CustomerID" type="C" required="true" default="20" desc="客户编号"/>
            <field name="SerialNo" client="SerialNo" type="C" required="true" default="20" desc="序列号"/>
            <field name="BeginDate" client="BeginDate" type="C" required="true" default="20" desc="开始日"/>
            <field name="EndDate" client="EndDate" type="C" required="true" default="20" desc="结束日"/>
            <field name="School" client="School" type="C" default="20" desc="所在学校"/>
            <field name="Department" client="Department" type="C" default="20" desc="所在院系"/>
            <field name="Specialty" client="Specialty" type="C" required="true" default="20" desc="专业"/>
            <field name="Degree" client="Degree" type="C" required="true" default="20" desc="最高学位"/>
            <field name="EduExperience" client="EduExperience" type="C" required="true" default="20" desc="  最高学历"/>
            <field name="SchoolLength" client="SchoolLength" type="C" required="true" default="20" desc="学制"/>
            <field name="DiplomaNo" client="DiplomaNo" type="C" required="true" default="20" desc="学历证书号"/>
            <field name="DegreeNo" client="DegreeNo" type="C" required="true" default="20" desc="学位证书号"/>
            <field name="InputUserName" client="InputUserName" type="C" required="true" default="20" desc="登记人"/>
            <field name="InputOrgName" client="InputOrgName" type="C" required="true" default="20" desc="登记机构"/>
            <field name="InputDate" client="InputDate" type="C" required="true" default="20" desc="登记日期"/>
            <field name="UpdateDate" client="UpdateDate" type="C" required="true" default="20" desc="更新日期"/>
        </field-list>
    </rcv>
</trans>
