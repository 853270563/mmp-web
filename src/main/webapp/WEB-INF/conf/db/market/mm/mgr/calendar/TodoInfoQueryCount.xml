<?xml version="1.0" encoding="utf-8"?>
<trans name="TodoInfoQueryCount" desc="金融日历客户意向记录数查询" ibatisStatement="INVEST_INFO.queryDayCount">
	<!-- 请求报文 -->
	<snd>
		<field name="USER_ID" length="20" type="C" desc="用户表示ID" required="true"/>
		<field name="INTE_DATE_BEGIN" length="10" type="C" desc="意向时间的开始时间" required="true"/>
		<field name="INTE_DATE_END" length="10" type="C" desc="意向时间的结束时间"  required="true"/>
	</snd>
	<!-- 响应报文 -->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field-list name="LIST" type="E">
			<field name="NUM" length="20" type="C" desc="记录数" />
			<field name="date"  xpath="INTE_DATE" length="60" type="C" desc="意向时间的时间" />
			
		</field-list>
	</rcv>
</trans>
