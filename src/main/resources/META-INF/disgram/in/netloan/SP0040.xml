<?xml version="1.0" encoding="utf-8"?>
<trans name="netloan/SP0040" excode="Image_APP01" desc="影像信息修改接口">
	<snd>
		<head>
			<field name="tranType" type="C" desc="交易类型" required="true" default="0"/>
			<field name="fileName" type="C" desc="文件名称" required="false"/>
		</head>
		<body>
			<field name="manager_code" length="32" type="C" desc="客户经理员工号" required="true" />						
			<field name="bar_code" length="32" type="C" desc="条码号" required="true"/>
			<field name="AMOUNT" length="32" type="C" desc="批次文件数量" required="true"/>
			<field name="BUSI_SERIAL_NO" length="32" type="C" desc="交易流水号" required="true"/>
			<field name="BUSI_START_DATE" length="80" type="C" desc="业务开始日期" required="true"/>
			<field name="LIST" type="E" desc="影像信息列表">
				<field name="BUSI_FILE_TYPE" length="15" type="C" desc="文件类型条码" required="true"/>
				<field name="BUSI_FILE_SCANUSER" length="10" type="C" desc="文件扫描人" required="true"/>
				<field name="BUSI_FILE_PAGENUM" length="2" type="C" desc="文件页码" required="true"/>
				<field name="FILE_CONTRACT_NO" length="100" type="C" desc="合同编号"/>
				<field name="FILE_CONSUME_NO" length="20" type="C" desc="消费编号"/>
				<field name="FILE_DEBT_NO" length="100" type="C" desc="借据编号"/>
				<field name="FILE_CN_NAME" length="10" type="C" desc="原文件名(图片ID)"/>
			</field>
		</body>	
	</snd>	
	<rcv>
		<head>
			<field name="txnCod" type="C" desc="交易代码"/>
			<field name="tranType" type="C" desc="交易类型"/>
			<field name="fileName" type="C" desc="文件名称"/>
			<field name="termDate" type="C" desc="交易日期"/>
			<field name="termTime" type="C" desc="交易时间"/>
		</head>
		<body>
			<field name="result" length="50" type="C" desc="结果"/>
			<field name="reason_fail" length="100" type="C" desc="失败原因" />
			<field name="manager_code" length="20" type="C" desc="客户经理员工号" />
			<field name="AMOUNT" length="32" type="C" desc="批次文件数量"/>
			<field name="BUSI_SERIAL_NO" length="32" type="C" desc="交易流水号"/>
			<field name="BUSI_START_DATE" length="80" type="C" desc="业务开始日期"/>
		</body>
	</rcv>
</trans>
