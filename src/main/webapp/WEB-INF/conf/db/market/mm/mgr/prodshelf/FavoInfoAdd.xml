<?xml version="1.0" encoding="utf-8"?>
<trans name="FavoInfoAdd" desc="添加收藏" ibatisStatement="PROD_FAVO.addFavoInfo">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<field name="PROD_ID" length="50" type="C" desc="收藏编号" client="prodId"/>
		<field name="PROD_TYPE" length="20" type="C" desc="收藏类型" client="prodType" />
		<field name="PROD_NAME" length="20" type="C" desc="收藏名称" client="prodName"/>
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field name="PROD_FAVO_ID" length="50" type="C" desc="收藏编号" />
	</rcv>
</trans>
