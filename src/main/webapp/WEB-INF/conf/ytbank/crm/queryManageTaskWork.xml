<?xml version="1.0" encoding="utf-8"?>
<trans name="queryManageTaskWork" desc="工作任务" serviceId="00850000000009">
    <!--请求报文 -->
    <snd>
        <field name="userId" length="100" type="C" desc="登录号" required="true"/>
    </snd>
    <!-- 响应报文 -->
    <rcv>
        <field name="item"  xpath="/Service/Service_Body/response/item" client="LIST" type="E"  desc="解析响应报文体">
            <field name="orgName" length="120" type="C" desc="机构名称 " required="true"/>
            <field name="orgId" length="20" type="C" desc="机构id " required="true"/>
            <field name="finishWorkNum" length="20" type="C" desc="已完成任务数 " required="true"/>
            <field name="overdueWorkNum" length="20" type="C" desc="逾期完成任务数 " required="true"/>
            <field name="untreatedWorkNum" length="20" type="C" desc="未处理任务数 " />
        </field>
    </rcv>
</trans>