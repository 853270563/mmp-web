<?xml version="1.0" encoding="utf-8"?>
<trans name="netloan/SP0032" excode="ApplyInfo_APP01" desc="客户申请信息修改">
	<snd>
		<head>
			<field name="tranType" type="C" desc="交易类型" required="true" default="0"/>
			<field name="fileName" type="C" desc="文件名称" required="false"/>
		</head>
		<body>
			<field name="manager_code" length="32" type="C" desc="客户经理员工号" required="true" />						
			<field name="bar_code" length="32" type="C" desc="条码号" required="true"/>
			<field name="credit_count" length="16" type="C" desc="授信总量" required="true"/>
			<field name="credit_count_timelimit" length="20" type="C" desc="授信总量期限" required="true"/>
			<field name="invest_loan_industry" length="10" type="C" desc="贷款行业投向"/>
			<field name="loop_loan_status" length="2" type="C" desc="自动循环贷款状态"/>
			<field name="loop_loan_lines" length="16" type="C" desc="自动循环额度" />
			<field name="loop_loan_timelimit" length="20" type="C" desc="自动循环额度期限"/>
			<field name="loop_loan_type" length="20" type="C" desc="自动循环-放款方式"/>
			<field name="loop_loan_use" length="20" type="C" desc="自动循环-贷款用途"/>
			<field name="loop_is_agriculture" length="2" type="C" desc="自助循坏-是否涉农"/>
			<field name="loop_agriculture_use" length="4" type="C" desc="自助循坏-涉农贷款用途" />
			<field name="single_loan_status" length="2" type="C" desc="单笔贷款状态"/>
			<field name="single_loan_lines" length="16" type="C" desc="单笔-贷款金额"/>
			<field name="single_loan_timelimit" length="20" type="C" desc="单笔-贷款期限"/>
			<field name="single_loan_type" length="20" type="C" desc="单笔-还款方式"/>
			<field name="single_loan_use" length="20" type="C" desc="单笔-贷款用途"/>
			<field name="single_is_agriculture" length="2" type="C" desc="单笔-是否涉农"/>
			<field name="single_agriculture_use" length="4" type="C" desc="单笔-涉农贷款用途"/>             
		</body>	
	</snd>	
	<rcv>
		<head>
			<field name="txnCod" type="C" desc="交易代码"/>
			<field name="tranType" type="C" desc="交易类型"/>
			<field name="fileName" type="C" desc="文件名称"/>
			<field name="termDate" type="C" desc="交易日期"/>
			<field name="termTime" type="C" desc="交易时间"/>
		</head>
		<body>
			<field name="result" length="50" type="C" desc="结果"/>
			<field name="manager_code" length="32" type="C" desc="客户经理员工号" />
			<field name="bar_code" length="32" type="C" desc="条码号"/>
			<field name="reason_fail" length="100" type="C" desc="失败原因"/>
		</body>
	</rcv>
</trans>
