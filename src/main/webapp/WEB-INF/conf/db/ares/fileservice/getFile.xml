<?xml version="1.0" encoding="utf-8"?>
<trans name="getFile" desc="单文件下载">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<field name="file" length="1000" required="true" type="C" desc="待下载文件名"/>
		<field name="token" length="100" required="false" type="C" desc="filename 的 sessionkey 加密的 16 进制串,采用16进制串的原因是 base64 串在 url 中会有 ‘=’ 及 ‘+’ 字符方面的问题"/>
	</snd>
	<!-- 响应报文 -->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
	</rcv>
</trans>
