<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="ARES_MP_MSG_TMPL">

	<select id="loadTmpl" parameterClass="map" resultClass="java.util.HashMap">
		Select
			t.*
		From
			${schema.configPlat}.ARES_MP_MSG_TMPL t
		Where
			t.TMPL_ID = #TMPLID#
	</select>
</sqlMap>

