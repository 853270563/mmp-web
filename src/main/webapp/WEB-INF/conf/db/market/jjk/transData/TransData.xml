<?xml version="1.0" encoding="utf-8"?>
<trans name="TransData" desc="进度查询统一入口"   ibatisStatement="TRANS_DATA.pageQuery">
	<snd>
		<field name="STARTDATE" type="C" desc="经办开始时间" required="false" />
		<field name="ENDDATE" type="C" desc="经办结束时间"  required="false"/>
		<field name="SIGN_TYPE" type="C" length="2" desc="业务类型" />
		<field name="SIGN_STATE" type="C" length="1" desc="业务状态" />
		<field name="NEXT_KEY" length="6" type="C" desc="起始记录号" />	
		<field name="PAGE_SIZE" length="6" type="C" desc="页记录数" />	
	</snd>
	<!-- 响应报文 -->
	<rcv> 
		<!--公共字段-->
		<field name="STATUS" length="6" type="C" desc="返回码" /> 
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field name="NEXT_KEY" length="6" type="C" desc="起始记录号" />	
		<field-list name="LIST" type="E"> 
			<field name="TRANS_ID" length="20" type="C" desc="流水ID" />
			<field name="CUST_NAME" length="20" type="C" desc="姓名" />
			<field name="CERT_NO" length="20" type="C" desc="身份证号" />
			<field name="SIGN_TYPE" length="2" type="C" desc="业务类型" />
			<field name="SIGN_TIME" length="20" type="C" desc="办理时间 " />
			<field name="SIGN_STATE" length="1" type="C" desc="当前状态" />
			<field name="SIGN_STATE_CN" length="20" type="C" desc="当前状态名称" />
		</field-list> 
	</rcv>
</trans>
