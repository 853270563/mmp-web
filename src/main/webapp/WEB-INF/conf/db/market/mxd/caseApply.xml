<?xml version="1.0" encoding="utf-8"?>
<trans name="caseApply" desc="贷款申请" ibatisStatement="CASE.insert">
    <snd>
        <field name="CUSTOMER_CERTIFICATE_TYPE" type="C" length="20" desc="证件类型" required="true"/>
        <field name="CUSTOMER_CERTIFICATE_NO" type="C" length="20" desc="证件号码" required="true"/>
        <field name="CUSTOMER_NAME" type="C" length="50" desc="客户名称"/>
        <!--<field name="CUSTOMER_ID" type="C" length="20" desc="客户编号" required="true"/>-->
        <field name="PRODUCT_ID" type="C" length="20" desc="产品编号" required="true"/>
        <field name="APPLY_AMOUNT" type="C" length="20" desc="贷款金额" required="true"/>
        <field name="LOGIN_NAME" type="C" length="20" desc="客户经理编号" required="true"/>
        <field name="USER_AUDIT_ID" type="C" length="20" desc="审批主管编号" required="true"/>
    </snd>

    <rcv>
        <field name="STATUS" length="6" type="C" desc="返回码"/>
        <field name="MSG" length="100" type="C" desc="返回消息"/>
    </rcv>
</trans>
