<?xml version="1.0" encoding="utf-8"?>
<trans name="queryZxList" desc="资讯信息">
    <!--请求报文 -->
    <snd>
        <!--报文头 -->
        <head>
            <field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000101014" desc="ESB 服务码"/>
            <field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
            <field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="816777777" desc="原始请求方机构代号 "/>
            <field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
            <field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
        </head>
        <!--报文体 -->
        <body>

            <!--请求体 -->
            <field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
                <field name="userId" length="20" type="C" desc="登录号(可不用)" required="true"/>
                <field name="snTitle" length="20" type="C" desc="标题" />
                <field name="snType" length="20" type="C" desc="类型" />
                <field name="status" length="20" type="C" desc="发布状态" />
                <field name="orgId" length="20" type="C" desc="机构号（可不用）" />
                <!--分页-->
                <field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
                    <field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="分页标非零0分页!0不分页" />
                    <field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
                    <field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="10"    desc="分页大小" />
                </field-extend>
            </field-request>
        </body>
    </snd>
    <!-- 响应报文 -->
    <rcv>
        <field name="status"     required="true" length="19"  xpath="Service_Header/service_response/status"   type="C" default="" desc=""/>
        <field name="code"     required="true" length="19"  xpath="Service_Header/service_response/code"   type="C" default="" desc=""/>
        <field name="desc"     required="true" length="19"  xpath="Service_Header/service_response/desc"   type="C" default="" desc=""/>
        <field name="requester_code"     required="true" length="19"  xpath="Service_Header/service_response/requester_code"   type="C" default="" desc=""/>
        <field name="requester_desc"     required="true" length="19"  xpath="Service_Header/service_response/requester_desc"   type="C" default="" desc=""/>
        <field name="service_sn"     required="true" length="19"  xpath="Service_Header/service_sn"   type="C" default="8167777770209011171" desc=""/>
        <field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000100002" desc="ESB 服务码"/>
        <field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
        <field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="816777777" desc="原始请求方机构代号 "/>
        <field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
        <field name="service_time"    required="true" length="2"   xpath="Service_Header/service_time"   type="C" default="39" desc="服务时间"/>
        <field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
        <field name="total"     required="true" length="10"   xpath="Service_Body/response/total"   type="C" default="01" desc="总数"/>
        <!--请求体 -->
        <field name="item"  xpath="/Service/Service_Body/response/item" client="LIST" type="E"  desc="解析响应报文体">
            <field name="snId " length="20" type="C" desc="公告编号 " required="true"/>
            <field name="snTitle" length="20" type="C" desc="标题" required="true"/>
            <field name="snType" length="20" type="C" desc="类型" required="true"/>
            <field name="snUserName" length="20" type="C" desc="发布人姓名" required="true"/>
            <field name="snCreateDate" length="20" type="C" desc="创建时间"/>
            <field name="snStatus" length="20" type="C" desc="发布状态" required="false"/>
            <field name="snContent" length="20" type="C" desc="内容" required="false"/>
            <field name="fileAdd" length="20" type="C" desc="附件ID" required="false"/>
            <field name="snDate" length="20" type="C" desc="发布时间" required="false"/>
            <field name="upFlag" length="20" type="C" desc="置顶标志(1置顶0不置顶)" required="false"/>
        </field>
    </rcv>
</trans>