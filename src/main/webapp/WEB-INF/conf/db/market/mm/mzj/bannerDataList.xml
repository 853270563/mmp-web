<?xml version="1.0" encoding="UTF-8"?>
<trans name="bannerDataList" ibatisStatement="MZJ_BANNER_DATA.bannerDataQuery" desc="广告banner列表">
	<!-- 请求报文 -->
	<snd>  
		<field name="ORGAN_LIMIT" length="20" type="C" desc="所属机构"/>
	</snd>
	<!-- 响应报文 -->
	<rcv> 
		<!--公共字段-->
		<field name="STATUS" length="6" type="C" desc="返回码" /> 
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field-list name="LIST" type="E">
			<field name="BANNER_ID" length="6" type="C" desc="广告Id"/>
			<field name="BANNER_NAME" length="6" type="C" desc="广告名称"/>
			<field name="BANNER_ORDE" length="6" type="C" desc="排序位置" />
			<field name="BANNER_LVL" length="6" type="C" desc="广告级别" />
			<field name="FILE_ID" length="6" type="C" desc="文件Id"/>
			<field name="FILE_TYPE" length="6" type="C" desc="文件类型"/>
			<field name="IMG_URL" length="100" type="C" desc="封面图片路径" />
			<field name="FILE_URL" length="100" type="C" desc="资源路径"/>
		</field-list>
	</rcv>
</trans>