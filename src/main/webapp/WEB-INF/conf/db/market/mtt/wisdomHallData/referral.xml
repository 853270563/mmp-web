<?xml version="1.0" encoding="utf-8"?>
<trans name="referral" desc="转介" ibatisStatement="">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<field name="RECEIVER" length="20" type="C" desc="接收人" />
		<field name="CUST_NO" length="20" type="C" desc="客户号" />
		<field name="CUST_NAME" length="20" type="C" desc="客户姓名" />
		<field name="MOBILE_NO" length="50" type="C" desc="联系手机" />
		<field name="ID_TYPE" length="20" type="C" desc="证件类型" />
		<field name="ID_NO" length="20" type="C" desc="证件号码" />
		<field name="PROD_TYPE" length="20" type="C" desc="产品类型" />
		<field name="PROD_NAME" length="50" type="C" desc="产品名称" />
		<field name="AMOUNT" length="20" type="C" desc="购买金额" />
		<field name="INTENT_TIME" length="20" type="C" desc="意向时间" />
		<field name="INTENT_STATUS" length="20" type="C" desc="意向状态" />
		<field name="REMARK" length="500" type="C" desc="备注" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!-- 公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
	</rcv>
</trans>
