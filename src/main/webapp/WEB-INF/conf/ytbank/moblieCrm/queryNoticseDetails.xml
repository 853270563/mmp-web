<?xml version="1.0" encoding="utf-8"?>
<trans name="querySysNotices" ibatisStatement="" desc="全部信息公告详情">
    <!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
    <snd>
        <!-- 客户信息  -->
        <field  name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"  />
        <!-- <field  name="pageIndex" length="30" type="C" desc="第几页"  />
        <field  name="pageSize" length="30" type="C" desc="每一页大小"  />  -->
        <field name="userId" length="20" type="C" desc="登录号(可不用)" required="true"/>
        <field name="snTitle" length="20" type="C" desc="标题" />
        <field name="snType" length="20" type="C" desc="类型" />
        <field name="status" length="20" type="C" desc="发布状态" />
        <field name="orgId" length="20" type="C" desc="机构号(可不用)" />
    </snd>
    <!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
    <rcv>
        <!--公共字段 -->
        <field name="STATUS" length="6" type="C" desc="返回码" />
        <field name="MSG" length="100" type="C" desc="返回消息" />
        <field name="SEQ" length="16" type="C" desc="流水号" />
        <field name="TRANS_TYPE" length="4" type="C" desc="交易类型" /><!-- 0001：新增客户进件信息 -->
        <field name="TRANS_CODE" length="4" type="C" desc="交易结果码" /><!-- 0000：表示交易成功 其它均为交易失败 -->
        <field name="TRANS_DESC" length="128" type="C" desc="交易结果描述" />
        <field name="TRANS_DATE" length="8" type="C" desc="响应日期" />
        <field name="TRANS_TIME" length="6" type="C" desc="响应时间" />

        <field-list name="item" type="E">
            <field name="snId " length="20" type="C" desc="公告编号 " required="true"/>
            <field name="snTitle" length="20" type="C" desc="标题" required="true"/>
            <field name="snType" length="20" type="C" desc="类型" required="true"/>
            <field name="snUserName" length="20" type="C" desc="发布人姓名" required="true"/>
            <field name="snCreateDate" length="20" type="C" desc="创建时间"/>
            <field name="snStatus" length="20" type="C" desc="发布状态" required="false"/>
            <field name="snContent" length="20" type="C" desc="内容" required="false"/>
            <field name="fileAdd" length="20" type="C" desc="附件ID" required="false"/>
            <field name="snDate" length="20" type="C" desc="发布时间" required="false"/>
            <field name="upFlag" length="20" type="C" desc="置顶标志(1置顶0不置顶)" required="false"/>
        </field-list>
    </rcv>
</trans>