<?xml version="1.0" encoding="utf-8"?>
<trans name="00080000200005" desc="客户列表查询">
	<!--请求报文 -->
	<snd>
				<!--分页-->
				<field name="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" /> 
				<field name="PAGE_SIZE"  type="C" required="true"  default="10"    desc="分页大小" />
				<field  name="CustomerName" length="80" type="C" desc="客户姓名 " />
				<field  name="CertID" length="40" type="C" desc="证件号码"  />
				<field  name="CustomerType" length="40" type="C" desc="客户类型"  mapKey="0110 公司客户
																																	0120 小企业客户
																																	0210 实体集团
																																	0220 虚拟集团
																																	0310 个人客户
																																	0410 同业客户
																																	0510 楼宇开发商
																																	0520 汽车经销商
																																	0530 担保公司
																																	0540 下岗失业担保客户
																																	0610 消费金融楼宇开发商
																																	0615 房抵贷合作商
																																	0620 消费金融汽车经销商
																																	0630 消费金融担保公司
																																	0640 消费金融下岗失业担保客户"/>
		
	</snd>
	<!-- 响应报文 -->
		<rcv>
				<field name="STATUS" length="6" type="C" desc="返回码" />
				<field name="MSG" length="100" type="C" desc="返回消息" />
				<field name="TOTAL_NUM"  type="C"  desc="总条数" />
				<field name="COUNT"  type="C"  desc="条数" />
   			    <field-list name="LIST"    type="E"  desc="解析响应报文体">
					<field name="CustomerName" length="16" type="C" desc="客户姓名" />
					<field name="CustomerID" length="4" type="C" desc="客户号" />
					<field name="CertID" length="4" type="C" desc="证件号码" />
					<field name="CellPhone" length="32" type="C" desc="手机号码" />
					<field name="CustomerTypeName" length="20" type="C" desc="客户类型" />
					<field name="InputUserName" length="20" type="C" desc="客户经理" />
					<field name="InputOrgName" length="20" type="C" desc="归属机构" />
				</field-list>
			
		</rcv>
</trans>
