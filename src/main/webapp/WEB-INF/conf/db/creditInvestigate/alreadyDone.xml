<?xml version="1.0" encoding="utf-8"?>
<trans name="alreadyDone" ibatisStatement="" desc="已办任务列表">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 客户信息  -->
		 <field  name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"  />
		<field  name="PAGE_NO" length="30" type="C" desc="第几页"  />
		<field  name="PAGE_SIZE" length="30" type="C" desc="每一页大小"  /> 
		<field  name="userId" length="10" type="C" desc="用户登录号" />
		<field name="customerName" client="customerName" type="C"    desc="客户名称" />  
		<field name="businessName" client="businessName" type="C"     desc="业务品种" />
		<field name="businessType" client="businessType" type="C"    desc="业务品种编码" /> 
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		 <!-- 代办记录 -->
        <field-list name="item" type="E">
           		<field name="serialNo" client="serialNo" type="C" required="true"  default="20"    desc="流程编号" />  
				<field name="applySerialNo" client="applySerialNo" type="C" required="true"  default="20"    desc="业务编号" />  
				<field name="objectType" client="objectType" type="C" required="true"  default="20"    desc="流程种类" />  
				<field name="businessType" client="businessType" type="C" required="true"  default="20"    desc="业务品种" />
				<field name="customerType" client="customerType" type="C" required="true"  default="20"    desc="客户类型" />  
				<field name="businessName" client="businessName" type="C" required="true"  default="20"    desc="业务品种汉语名字" />
				<field name="customerID" client="customerID" type="C" required="true"  default="20"    desc="客户编号" />  
				<field name="customerName" client="customerName" type="C"  default="20"    desc="客户名称" />  
				<field name="inputUserName" client="inputUserName" type="C"   default="20"    desc="客户经理" />  
				<field name="inputOrgName" client="inputOrgName" type="C" required="true"  default="20"    desc="客户经理所在行" />  
				<field name="businessName" client="businessName" type="C" required="true"  default="20"    desc="业务品种" />  
				<field name="beginTime" client="beginTime" type="C" required="true"  default="20"    desc="开始时间" />  
				<field name="endTime" client="endTime" type="C" required="true"  default="20"    desc="结束时间" />  
				<field name="phaseName" client="phaseName" type="C" required="true"  default="20"    desc="当前阶段名称" />
				<field name="updateDate" client="updateDate" type="C" required="true"  default="20"    desc="修改时间" />
				<field name="flowNo" client="flowNo" type="C" required="true"  default="20"    desc="流程号" />
				<field name="phaseNo" client="phaseNo" type="C" required="true"  default="20"    desc="阶段号" />  
				<field name="applyType" client="applyType" type="C" required="true"  default="20"    desc="申请类型" /> 
        </field-list>
	</rcv>
</trans>
	