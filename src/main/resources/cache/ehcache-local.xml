<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<ehcache updateCheck="false" name="defaultCache">

	<diskStore path="java.io.tmpdir/jeesite/ehcache/default" />

	<!-- DefaultCache setting. -->
	<defaultCache maxElementsInMemory="500" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="false" />

    <cache name="sysCache" maxElementsInMemory="500" eternal="true" overflowToDisk="false"/>

    <cache name="SimplePageCachingFilter" maxElementsInMemory="100" eternal="false" overflowToDisk="false"
    	timeToIdleSeconds="120" timeToLiveSeconds="120" memoryStoreEvictionPolicy="LFU"/>
    	
</ehcache>
