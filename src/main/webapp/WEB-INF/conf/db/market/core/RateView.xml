<?xml version="1.0" encoding="utf-8"?>
<trans name="RateView" desc="利率" ibatisStatement="RATE_INTE.queryRate">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<!--过滤响应信息，并对数据字典进行解析-->
		<field name="PROD_TYPE_CODE" length="20" type="C" desc="业务类型"/>
		<field name="PROD_MONT_YEAR" length="20" type="C" desc="业务年限"/>
		
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析 -->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field-list name="LIST" type="E">
			<field name="PROD_TYPE_CODE" length="10" type="C" desc="贷款利率编号" />
			<field name="PROD_RATE" length="10" type="N" desc="贷款利率" />
			<field name="PROD_MINI_AMT" length="10" type="N" desc="最低贷款额 " />
			<field name="PROD_MONT_YEAR" length="20" type="C" desc="业务年限"/>
			<field name="PROD_MINI_DESC" length="100" type="C" desc="最低业务金额"/>
			<field name="PROD_DESC" length="100" type="C" desc="业务介绍"/>
			<field name="PROD_CLASS" length="20" type="C" desc="存取款类型"/>
		</field-list>
	</rcv>
</trans>
