<?xml version="1.0" encoding="utf-8"?>
<trans name="projectLicenseInfo" ibatisStatement="" desc="项目许可证信息">
    <!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
    <snd>
        <!-- 项目许可证信息  -->
        <field name="controlFlag" length="30" type="C" desc="是否分页，0不分页，非0分页"/>
        <field name="apply_no" client="applySerialNo" type="C" required="true" default="" desc="业务流程编号"/>
    </snd>
    <!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
    <rcv>
        <!--公共字段 -->
        <field name="STATUS" length="6" type="C" desc="返回码"/>
        <field name="MSG" length="100" type="C" desc="返回消息"/>
        <field name="SEQ" length="16" type="C" desc="流水号"/>
        <field name="TRANS_TYPE" length="4" type="C" desc="交易类型"/><!-- 0001：新增客户进件信息 -->
        <field name="TRANS_CODE" length="4" type="C" desc="交易结果码"/><!-- 0000：表示交易成功 其它均为交易失败 -->
        <field name="TRANS_DESC" length="128" type="C" desc="交易结果描述"/>
        <field name="TRANS_DATE" length="8" type="C" desc="响应日期"/>
        <field name="TRANS_TIME" length="6" type="C" desc="响应时间"/>
        <!-- 代办记录 -->
        <field-list name="LIST" type="E">
            <field name="ObjectType" client="ObjectType" type="C" required="true" default="20" desc="对象类型"/>
            <field name="ObjectNo" client="ObjectNo" type="C" required="true" default="20" desc="申请编号"/>
            <field name="SerialNo" client="SerialNo" type="C" required="true" default="20" desc="流水号"/>
            <field name="LCType" client="LCType" type="C" required="true" default="20" desc="许可证类型"/>
            <field name="LCNo" client="LCNo" type="C" default="20" desc="许可证编号"/>
            <field name="Flag1" client="Flag1" type="C" default="20" desc="项目状况"/>
            <field name="LoadingDate" client="LoadingDate" type="C" required="true" default="20" desc="发证日"/>
            <field name="ValidDate" client="ValidDate" type="C" required="true" default="20" desc="到期日"/>
            <field name="UserName" client="UserName" type="C" required="true" default="20" desc="  登记人"/>
            <field name="OrgName" client="OrgName" type="C" required="true" default="20" desc="登记机构"/>
        </field-list>
    </rcv>
</trans>