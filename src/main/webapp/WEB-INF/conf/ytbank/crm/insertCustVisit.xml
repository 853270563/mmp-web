<?xml version="1.0" encoding="utf-8"?>
<trans name="insertCustVisit" desc="客户访谈详细信息保存">
  <snd>
                <field name="twTitle" length="120" type="C" desc="任务标题" required="true"/>
                <field name="twBeginDate" length="20" type="C" desc="任务开始时间" required="true"/>
                <field name="twEndDate" length="20" type="C" desc="任务结束时间" required="true"/>
                <field name="visitType" length="20" type="C" desc="访谈类型"/>
                <field name="twCont" length="20" type="C" desc="任务内容" required="true"/>
                <field name="visitChannel" length="20" type="C" desc="访谈方式" />
                <field name="visitDate" length="20" type="C" desc="访谈时间" />
                <field name="visitDesc" length="20" type="C" desc="访谈内容" />
    </snd>
    <!-- 响应报文 -->
    <rcv>
        <field name="status"     required="true" length="19"  xpath="Service_Header/service_response/status"   type="C" default="" desc=""/>
        <field name="code"     required="true" length="19"  xpath="Service_Header/service_response/code"   type="C" default="" desc=""/>
        <field name="desc"     required="true" length="19"  xpath="Service_Header/service_response/desc"   type="C" default="" desc=""/>
    </rcv>
</trans>