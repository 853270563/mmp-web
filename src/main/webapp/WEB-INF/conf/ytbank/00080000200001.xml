<?xml version="1.0" encoding="utf-8"?>
<trans name="00080000200001" desc="贷款申请进度列表查询">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000200001" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="816777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
			
				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field  name="CustomerName" length="10" type="C" desc="客户姓名 " default="{CustomerName}" required="true"/>
				<field  name="UserID" length="10" type="C" desc="用户编号" client="USER_ID" default="{UserID}" required="true"/>
				<field  name="CertID" length="10" type="C" desc="证件号码" default="{CertID}" required="true"/>
				<field  name="BeginDate" length="10" type="C" desc="开始时间" default="{BeginDate}" required="true"/>
				<field  name="EndDate" length="10" type="C" desc="结束时间" default="{EndDate}" required="true"/> 
				<!--分页-->
				<field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
					<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="1分页0不分页" />
					<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
					<field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="10"    desc="分页大小" />
				</field-extend>
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<head>
   		    	<field-response name="service_response"  xpath="Service_Header/service_response" type="S" desc="解析响应报文头">
   		 			<field name="status" client="status" type="C" length="19"  	desc="交易状态"  />
					<field name="code"	client="code"   type="C" length="14"   required="true" 	desc="提供方响应码"   />
					<field name="desc" 	client="desc" 	 type="C" length="200" required="true"  desc="提供方响应描述"  />
   		   	 	</field-response>
   			</head>
   			<body>
   				
				<field name="TOTAL_NUM"  type="C" xpath="Service_Body/response/total" desc="总条数" />
				<field name="COUNT"  type="C" xpath="Service_Body/response/count" desc="条数" />
   			    <field-response name="LIST"  xpath="Service_Body/response/item"  type="E"  desc="解析响应报文体">
					<field name="TaskSerialNo" length="16" type="C" desc="任务流水号" />
					<field name="ApplySerialNo" length="4" type="C" desc="业务流水号" />
					<field name="BusinessName" length="4" type="C" desc="业务类型" />
					<field name="BusinessType" length="32" type="C" desc="业务类型代码" />
					<field name="CustomerName" length="8" type="C" desc="客户名称" />
					<field name="CertType" length="20" type="C" desc="证件类型代码" />
					<field name="CertID" length="20" type="C" desc="证件号码" />
					<field name="CellPhone" length="20" type="C" desc="手机号码" />
					<field name="BusinessSum" length="20" type="C" desc="贷款金额" />
					<field name="PhaseName" length="20" type="C" desc="当前业务阶段" />
					<field name="PhaseNo" length="20" type="C" desc="阶段号" />
					<field name="PhaseType" length="20" type="C" desc="阶段类型" />
					<field name="InputUserName" length="20" type="C" desc="客户经理" />
					<field name="InputOrgName" length="20" type="C" desc="归属机构" />
				</field-response>
			</body>
		</rcv>
</trans>
