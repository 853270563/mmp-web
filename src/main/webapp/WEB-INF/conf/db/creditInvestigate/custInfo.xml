<?xml version="1.0" encoding="utf-8"?>
<trans name="custInfo" ibatisStatement="" desc="公司、小企业客户概况">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 客户信息  -->
		<field name="customer_id" client="customerId" type="C"  required="true"    desc="客户编号" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		 <!-- 代办记录 -->
        <field-list name="item" type="E">
            		<field name="serialNo" client="serialNo" type="C" required="true"  default="20"    desc="业务流程号" />  
					<field name="enterpriseName" client="enterpriseName" type="C" required="true"  default="20"    desc="客户名称" />  
					<field name="industryType" client="industryType" type="C" required="true"  default="20"    desc="国标行业分类" />  
					<field name="customerID" client="customerID" type="C" required="true"  default="20"    desc="客户编号" />  
					<field name="corpID" client="corpID" type="C"  default="20"    desc="组织机构代码" />  
					<field name="certDate" client="certDate" type="C"   default="20"    desc="组织机构登记/年检/更新日期" />  
					<field name="licenseNo" client="licenseNo" type="C" required="true"  default="20"    desc="工商营业执照号码" />  
					<field name="orgNature" client="orgNature" type="C" required="true"  default="20"    desc="发生类型" />  
					<field name="licenseDate" client="licenseDate" type="C" required="true"  default="20"    desc="  营业执照成立日" />  
					<field name="licenseMaturity" client="licenseMaturity" type="C" required="true"  default="20"    desc="营业执照到期日" />  
					<field name="registerAdd" client="registerAdd" type="C" required="true"  default="20"    desc="注册地址" />
					<field name="mostBusiness" client="mostBusiness" type="C" required="true"  default="20"    desc="经营范围 "/>
					<field name="registerCapital" client="registerCapital" type="C" required="true"  default="20"    desc=" 注册资本"/>
					<field name="paiclUpCapital" client="paiclUpCapital" type="C" required="true"  default="20"    desc=" 实收资本"/>  
					<field name="orgType" client="orgType" type="C" required="true"  default="20"    desc=" 企业类型"/>  
					<field name="listingCorpOrNot" client="listingCorpOrNot" type="C" required="true"  default="20"    desc="上市公司类型 "/>  
					<field name="captType" client="captType" type="C" required="true"  default="20"    desc=" 企业出资人成分"/>  
					<field name="isGroup" client="isGroup" type="C" required="true"  default="20"    desc=" 是否集团客户"/>  
					<field name="hasIERight" client="hasIERight" type="C" required="true"  default="20"    desc=" 有无进出口经营权"/>  
					<field name="isHghTec" client="isHghTec" type="C" required="true"  default="20"    desc=" 是否高新技术企业"/>
					<field name="isGFRZ" client="isGFRZ" type="C" required="true"  default="20"    desc=" 是否政府融资平台"/>  
        </field-list>
	</rcv>
</trans>
	