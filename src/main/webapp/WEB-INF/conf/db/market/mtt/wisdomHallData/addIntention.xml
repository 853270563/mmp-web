<?xml version="1.0" encoding="utf-8"?>
<trans name="addIntention" desc="意向添加" ibatisStatement="">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
	<snd>
		<field name="RECEIVER_ID" length="20" type="C" desc="意向处理人" />
		<field name="CUST_NAME" length="20" type="C" desc="客户名称" />
		<field name="MOBILE_NO" length="20" type="C" desc="手机号码" />
		<field name="ID_TYPE" length="2" type="C" desc="证件类型" />
		<field name="ID_NO" length="20" type="C" desc="证件号码" />
		<field name="PROD_ID" length="20" type="C" desc="产品编号" />
		<field name="AMOUNT" length="10" type="C" desc="购买金额" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析 -->
	<rcv>
		<!-- 公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
	</rcv>
</trans>
