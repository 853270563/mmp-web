<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.yitong.core.dao.DictDao">
	
	<!-- 查询菜单数据-->  
    <select id="getDictValuesFromDb" parameterType="map" resultType="cn.com.yitong.core.model.SysDict">
       select LABEL,VALUE,TYPE,DESCRIPTION from SYS_DICT where TYPE = #{type} and DEL_FLAG='0'
    </select>	
</mapper>
