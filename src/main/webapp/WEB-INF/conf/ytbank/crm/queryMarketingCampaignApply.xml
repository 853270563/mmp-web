<?xml version="1.0" encoding="utf-8"?>
<trans name="queryMarketingCampaignApply" desc="营销活动报名" serviceId="00850000000060">
    <!--请求报文 -->
    <snd>
        <field name="mkActId" length="120" type="C" desc="营销活动ID" required="true"/>
        <field name="mkActName" length="20" type="C" desc="营销活动名称" required="true"/>
        <field name="userId" length="100" type="C" desc="登录号" required="true"/>
        <field name="mkActZt" length="20" type="C" desc="状态：1 未提交 2 已提交" required="true"/>
    </snd>
    <!-- 响应报文 -->
    <rcv>
        <field name="item"  xpath="/Service/Service_Body/response/item" client="LIST" type="E"  desc="解析响应报文体">
            <field name="custName" length="120" type="C" desc="客户姓名" required="true"/>
            <field name="custId" length="20" type="C" desc="客户id" required="true"/>
            <field name="mkActZt" length="20" type="C" desc="状态：1 未提交 2 已提交" required="true"/>
            <field name="custGrade" length="20" type="C" desc="客户等级" required="true"/>
        </field>
    </rcv>
</trans>