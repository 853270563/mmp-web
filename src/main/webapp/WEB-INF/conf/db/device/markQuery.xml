<?xml version="1.0" encoding="utf-8"?>
<trans name="markQuery" ibatisStatement="MZJ_RES_DATA.pageQuery"
       desc="设备标记(黑白名单)查询">
    <!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换 -->
    <snd>
        <field name="UDID" length="20" required="true" type="C" desc="设备唯一标识" />
        <field name="MARK_TYPE" length="1" required="true" type="C" desc="标记类型(0=黑名单,1=白名单)" />
    </snd>
    <!-- 响应报文，可过滤响应信息，并对数据字典进行解析 -->
    <rcv>
        <!--公共字段 -->
        <field name="STATUS" length="6" type="C" desc="返回码" />
        <field name="MSG" length="100" type="C" desc="返回消息" />

        <field-list name="APP_LIST" type="E">
            <field name="APP_ID" length="20" type="C" desc="应用编号" />
            <field name="APP_NAME" length="20" type="C" desc="应用名称" />
            <field name="APP_VERSION" length="20" type="C" desc="应用版本" />
            <field name="APP_ID" length="20" type="C" desc="校验码" />
            <field name="APP_SHA1" length="20" type="C" desc="标记类型(0=黑名单,1=白名单)" />
        </field-list>
    </rcv>
</trans>
