<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="SEQ">
	<!-- 获得某一序列号的next值 -->
	<select id="nextVal" parameterClass="String" remapResults="true" resultClass="int">
		select $seqName$.NEXTVAL from SYSIBM.SYSDUMMY1
	</select> 

	<select id="P_TRANS_LOGS_SEQ" resultClass="int">
		select ${schema.configPlat}.P_TRANS_LOGS_SEQ.NEXTVAL from SYSIBM.SYSDUMMY1
	</select>

	<!-- 日誌主鍵 -->
	<select id="SEQ_OP" resultClass="int">
		select ${schema.configPlat}.SEQ_OP.NEXTVAL from SYSIBM.SYSDUMMY1
	</select>

	<select id="SEQ_ERR_CODE" resultClass="int">
		select ${schema.configPlat}.SEQ_PUB.NEXTVAL from SYSIBM.SYSDUMMY1
	</select> 
</sqlMap>

