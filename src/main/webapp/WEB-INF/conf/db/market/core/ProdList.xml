<?xml version="1.0" encoding="utf-8"?>
<trans name="ProdList" ibatisStatement="MZJ_PROPA_DATA.pageQuery" desc="产品货架信息列表">
	<!-- 请求报文 -->
	<snd>  
		<field name="NEXT_KEY" length="6" type="C" desc="起始记录号" />	
		<field name="PAGE_SIZE" length="6" type="C" desc="页记录数" />	
	</snd>
	<!-- 响应报文 -->
	<rcv> 
		<!--公共字段-->
		<field name="STATUS" length="6" type="C" desc="返回码" /> 
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<!-- 下次查询的起始记录号，为空表示没有下页 -->
		<field name="NEXT_KEY" length="6" type="C" desc="起始记录号" />	
		<field-list name="LIST" type="E"> 
			<field name="RID" length="6" type="C" desc="产品编号" xpath="PROPA_ID"/>	
			<field name="PROD_NAME" length="6" type="C" desc="产品名称" xpath="PROPA_NAME"/>	
			<field name="PROD_TYP" length="6" type="C" desc="产品分类" xpath="PROPA_TYPE"/>	
			<field name="PROD_MEMO" length="6" type="C" desc="产品说明" xpath="FILE_NAME"/>	
			<field name="FILE_ID" length="100" type="C" desc="资源编号" />
			<field name="FILE_URL" length="100" type="C" desc="资源路径" xpath="FILE_ADDR"/>
			<field name="FILE_TYP" length="100" type="C" desc="资源类型" />
			<field name="IMG_ID" length="100" type="C" desc="封面图片"/>
			<field name="IMG_URL" length="100" type="C" desc="封面图片" xpath="PROPA_PIC"/>
			<field name="AUDI_ID" length="6" type="C" desc="版本号" />		
		</field-list> 
	</rcv>
</trans>
