<?xml version="1.0" encoding="utf-8"?>
<trans name="busiQuery" desc="卡验密">
	<snd>
		<field name="bankNo" length="4" type="C" desc="分行号"/>
		<field name="idNBR" length="18" type="C" required="true" desc="证件号码"/>
		<field name="custName" length="30" type="C" desc="姓名"/>
		<field name="barCode" length="20" type="C" desc="条形码"/>
		<field name="barCode" length="20" type="C" desc="条形码"/>
		<field name="sendCode" length="20" type="C"  required="true" desc="验证码"/>
	</snd>
	<rcv>
	   <!--公共字段 -->
		<field name="errorCode" length="4" type="C" desc="响应码" />
		<field name="errorMsg" length="20" type="C" desc="响应描述" />
		<field name="appCount" length="20" type="C" desc="申请资料记录数" />
		
