<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="PushProcessAct">

    <sql id="BaseColumnList">
        PROCESS_INST_ID,ID,ACT_TYPE,ACT_NAME,AUDIT_ACTION,
        AUDIT_CONTENT, AUDIT_TIME,AUDIT_USER,CREATE_DATE,UPDATE_DATE
    </sql>
    <sql id="BaseColumnList2">
        PROCESS_INST_ID,ID,ACT_TYPE,ACT_NAME,AUDIT_ACTION,
        AUDIT_CONTENT,to_char(AUDIT_TIME,'yyyy-mm-dd hh24:mi:ss') as AUDIT_TIME,AUDIT_USER,CREATE_DATE,UPDATE_DATE
    </sql>

    <sql id="BaseColumnValueList">
        #{PROCESS_INST_ID},#{ID},#{ACT_TYPE},#{ACT_NAME},#{AUDIT_ACTION},
        #{AUDIT_CONTENT},#{AUDIT_TIME},#{AUDIT_USER},#{CREATE_DATE},#{UPDATE_DATE}
    </sql>

    <sql id="TableClause"> ${schema.busiPlat}.PUSH_PROCESS_ACT </sql>

    <sql id="WhereClause4Id">
        <where>
            ID = #{ID}
        </where>
    </sql>

    <insert id="insert" parameterType="map">
        insert into <include refid="TableClause" /> (
        <include refid="BaseColumnList" />
        ) values (
        <include refid="BaseColumnValueList" />
        )
    </insert>

    <select id="queryById" parameterType="string" resultType="map">
        <include refid="public.SelectById" />
    </select>

    <update id="updateById" parameterType="map">
        update <include refid="TableClause" />
        <set>
        ROCESS_INST_ID=#{ROCESS_INST_ID},
        ACT_TYPE=#{ACT_TYPE},
        ACT_NAME=#{ACT_NAME},
        AUDIT_ACTION=#{AUDIT_ACTION},
        AUDIT_CONTENT=#{AUDIT_CONTENT},
        AUDIT_TIME=#{AUDIT_TIME},
        AUDIT_USER=#{AUDIT_USER},
        CREATE_DATE=#{CREATE_DATE},
        UPDATE_DATE=#{UPDATE_DATE}
        </set> <include refid="WhereClause4Id" />
    </update>

     <update id="updateByIdSelective" parameterType="map">
            update <include refid="TableClause" />
            <set>
                <if test="ROCESS_INST_ID!=null and ROCESS_INST_ID!=''">ROCESS_INST_ID=#{ROCESS_INST_ID},</if>
                <if test="ACT_TYPE!=null and ACT_TYPE!=''">ACT_TYPE=#{ACT_TYPE},</if>
                <if test="ACT_NAME!=null and ACT_NAME!=''">ACT_NAME=#{ACT_NAME},</if>
                <if test="AUDIT_ACTION!=null and AUDIT_ACTION!=''">AUDIT_ACTION=#{AUDIT_ACTION},</if>
                <if test="AUDIT_CONTENT!=null and AUDIT_CONTENT!=''">AUDIT_CONTENT=#{AUDIT_CONTENT},</if>
                <if test="AUDIT_TIME!=null and AUDIT_TIME!=''">AUDIT_TIME=#{AUDIT_TIME},</if>
                <if test="AUDIT_USER!=null and AUDIT_USER!=''">AUDIT_USER=#{AUDIT_USER},</if>
                <if test="CREATE_DATE!=null and CREATE_DATE!=''">CREATE_DATE=#{CREATE_DATE},</if>
                <if test="UPDATE_DATE!=null and UPDATE_DATE!=''">UPDATE_DATE=#{UPDATE_DATE}</if>
            </set> <include refid="WhereClause4Id" />
     </update>


    <update id="deleteById" parameterType="string">
        <include refid="public.DeleteById" />
    </update>

    <select id="queryByCriteria" resultType="map" parameterType="map">
        <include refid="public.Select4Query" />
    </select>

    <select id="countByCriteria" resultType="int" parameterType="map">
        <include refid="public.Count4Query" />
    </select>

    <select id="deleteByCriteria" resultType="int" parameterType="map">
        <include refid="public.Delete4Query" />
    </select>
</mapper>
