<?xml version="1.0" encoding="utf-8"?>
<trans name="00080000200003"  desc="预警事项查询" >
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000200003" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="816777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
			
				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field  name="CustomerName" length="80" type="C" desc="客户姓名 " default="{CustomerName}" required="true"/>
				<field  name="CertID" length="40" type="C" desc="证件号码"  default="{CertID}" required="true"/>
				<field  name="SignalType" length="40" type="C" desc="预警类型"  default="{SignalType}" required="true" comment="01 一半预警02 重大预警"/>
				<field  name="BelongManDepart" length="40" type="C" desc="归属管理部门"  default="{BelongManDepart}" required="true" comment="0310 个人业务部0110 公司业务部0120 小企业贷款部"/>
				<field  name="UserID" length="40" type="C" desc="用户编号" client="USER_ID" default="{UserID}" required="true" />
				<!--分页-->
				<field-extend name="page_control" type="S" desc="分页">
					<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="1分页0不分页" />
					<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
					<field name="pagesize" client="pageSize" type="C" required="true"  default="10"    desc="分页大小" />
				</field-extend>
				
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<head>
   		    	<field-response name="service_response"  xpath="Service_Header/service_response" type="S" desc="解析响应报文头">
   		 			<field name="status" client="status" type="C" length="19"  	desc="交易状态"  />
					<field name="code"	client="code"   type="C" length="14"   required="true" 	desc="提供方响应码"   />
					<field name="desc" 	client="desc" 	 type="C" length="200" required="true"  desc="提供方响应描述"  />
   		   	 	</field-response>
   			</head>
   			<body>
   				<field name="TOTAL_NUM"  type="C" xpath="Service_Body/response/total" desc="总条数" />
   				<field name="COUNT"  type="C" xpath="Service_Body/response/count" desc="条数" />
   			    <field-response name="LIST"  xpath="Service_Body/response/item"  type="E"  desc="解析响应报文体">
					<field name="CustomerName" length="16" type="C" desc="客户姓名" />
					<field name="SerialNo" length="4" type="C" desc="预警信息流水号" />
					<field name="CertName" length="4" type="C" desc="证件名称" />
					<field name="CertID" length="32" type="C" desc="证件号码" />
					<field name="MessageContent" length="8" type="C" desc="预警信号" />
					<field name="SignalType" length="20" type="C" desc="归属类型" />
					<field name="BelongManDepart" length="20" type="C" desc="归属管理部门" />
					<field name="InputUserName" length="20" type="C" desc="登记人" />
					<field name="InputOrgName" length="20" type="C" desc="登记人归属机构" />
					<field name="InputDate" length="20" type="C" desc="登记时间" />
					<field name="UpdateDate" length="20" type="C" desc="更新时间" />
				</field-response>
			</body>
		</rcv>
</trans>
