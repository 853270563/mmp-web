<?xml version="1.0" encoding="utf-8"?>
<trans name="CardPassReset" desc="卡密码重置">
	<snd>
		<field name="bzh" length="2" type="C" desc="币种号" required="true"/>
		<field name="czy" length="4" type="C" desc="操作员" required="true"/>
		<field name="jgm" length="9" type="C" desc="机构码" required="true"/>
		<field name="sblx" length="1" type="C" desc="设备类型" required="true"/>
		<field name="jym" length="4" type="C" desc="交易码" required="true"/>
		<field name="sqczy" length="4" type="C" desc="授权操作员" />
		<field name="sqczymm" length="19" type="C" desc="授权操作员密码" />
		<field name="kjz" length="1" type="C" desc="卡介质" />
		<field name="kh" length="19" type="C" desc="卡号" />
		<field name="pzzl" length="2" type="C" desc="凭证种类" />
		<field name="zjhm" length="1" type="C" desc="证件号码" />
		<field name="zjzl" length="8" type="C" desc="证件种类" />
		<field name="jmzl" length="16" type="C" desc="加密种类" />
		<field name="mmj" length="8" type="C" desc="旧密码" />
		<field name="jmmmj" length="16" type="C" desc="加密旧密码" />
