<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.yitong.portal.thirdApp.dao.AresAuthorityDao">

    <select id="queryAppInfoByAppId" parameterType="string" resultType="map">
        SELECT T.* FROM ${schema.configPlat}.ARES_APP_INFO T WHERE T.APP_ID = #{appId}
    </select>

</mapper>
