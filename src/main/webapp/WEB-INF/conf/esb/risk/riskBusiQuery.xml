<?xml version="1.0" encoding="utf-8"?>
<trans name="riskBusiQuery" desc="预警事项查询">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080001000002" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="0018" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
			<!--分页-->
			<field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
				<field name="control_flag" client="control_flag" type="C"  default="1" desc="分页标非零0分页!0不分页" />
				<field name="pageindex" client="pageIndex" type="C"  default="1"    desc="分页角码" />
				<field name="pagesize" client="pageSize" type="C" default="10"    desc="分页大小" />
			</field-extend>
				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field name="cust_name" client="cust_name" type="C"   default="30"    desc="客户姓名" />
				<field name="risk_type" client="risk_type" type="C"   default="1"    desc="预警类型	" />  
				<field name="mng_dptm" client="mng_dptm" type="C"   default=""    desc="归属管理部门" /> 
				<field name="register" client="register" type="C"   default=""    desc="登记人" /> 
				<field name="regist_org" client="regist_org" type="C"    default=""    desc="登记机构" /> 
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<head>
   		    	<field-response name="service_response"  xpath="Service_Header/service_response" type="S" desc="解析响应报文头">
   		 			<field name="status" client="status" type="C" length="19"  	desc="交易状态"  />
					<field name="code"	client="code"   type="C" length="14"   required="true" 	desc="提供方响应码"   />
					<field name="desc" 	client="desc" 	 type="C" length="200" required="true"  desc="提供方响应描述"  />
   		   	 	</field-response>
   			</head>
   			<body name="body">
   			    <field-response name="response"  xpath="Service_Body/response"  type="S"  desc="解析响应报文体">
					<field-list name="list"   type="E"  >
						<field name="cust_name" client="cust_name" type="C" required="true"  default="20"    desc="客户姓名" />  
						<field name="cert_name" client="cert_name" type="C" required="true"  default="20"    desc="证件名称" />  
						<field name="cert_no" client="cert_no" type="C" required="true"  default="20"    desc="证件号码" />  
						<field name="risk_sign" client="risk_sign" type="C" required="true"  default="20"    desc="预警信号" />
						<field name="risk_type" client="risk_type" type="C" required="true"  default="20"    desc="预警类型" />  
						<field name="mng_dptm" client="mng_dptm" type="C" required="true"  default="20"    desc="归属管理部门" />
						<field name="register" client="register" type="C"  required="true"  default=""    desc="登记人" /> 
						<field name="regist_org" client="regist_org" type="C"  required="true"  default=""    desc="登记机构" /> 
						<field name="regist_date" client="regist_date" type="C"  required="true"  default=""    desc="登记时间" /> 
						<field name="upd_date" client="upd_date" type="C"  required="true"  default=""    desc="更新时间" /> 
						<field name="risk_status" client="risk_status" type="C"  required="true"  default=""    desc="预警状态" /> 
					</field-list>
				</field-response>
			</body>
		</rcv>
</trans>