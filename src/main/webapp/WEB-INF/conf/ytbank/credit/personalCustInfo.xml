<?xml version="1.0" encoding="utf-8"?>
<trans name="personalCustInfo" desc="个人客户信息">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000100020" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
  				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field name="customer_id" client="customerId" type="C"  required="true"  default=""    desc="客户编号" />
			<field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
				<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="分页标非零0分页!0不分页" />
				<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
				<field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="10"    desc="分页大小" />
			</field-extend>   
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<field name="status"     required="true" length="19"  xpath="Service_Header/service_response/status"   type="C" default="" desc=""/>
			<field name="code"     required="true" length="19"  xpath="Service_Header/service_response/code"   type="C" default="" desc=""/>
			<field name="desc"     required="true" length="19"  xpath="Service_Header/service_response/desc"   type="C" default="" desc=""/>
			<field name="total"     required="true" length="10"   xpath="Service_Body/response/total"   type="C" default="01" desc="总数"/>
			<!-- desc 格式说明    业务品种，字段含义/业务品种，字段含义-->
   			<field name="item"  xpath="Service_Body/response/item"  type="E"  desc="解析响应报文体">
					<field name="CustomerID" client="CustomerID" type="C" required="true"  default="20"    desc="客户编号" />  
					<field name="MFCustomerID" client="MFCustomerID" type="C" required="true"  default="20"    desc="核心客户号" />  
					<field name="CustomerName" client="CustomerName" type="C" required="true"  default="20"    desc="姓名" />  
					<field name="Sex" client="Sex" type="C" required="true"  default="20"    desc="性别" />  
					<field name="Nationality" client="Nationality" type="C"  default="20"    desc="民族" />  
					<field name="CertType" client="CertType" type="C"   default="20"    desc="证件类型" />  
					<field name="CertID" client="CertID" type="C" required="true"  default="20"    desc="证件号码" />  
					<field name="CertValidDate" client="CertValidDate" type="C" required="true"  default="20"    desc="身份证有效期" />  
					<field name="CertAddress" client="CertAddress" type="C" required="true"  default="20"    desc="  身份证地址" />  
					<field name="SINo" client="SINo" type="C" required="true"  default="20"    desc="社会保险号" />  
					<field name="IndType" client="IndType" type="C" required="true"  default="20"    desc="客户类型" />
					<field name="IsGroup" client="IsGroup" type="C" required="true"  default="20"    desc="是否集团客户 "/>
					<field name="EduExperienceEduExperience" client="EduExperience" type="C" required="true"  default="20"    desc=" 最高学历"/>
					<field name="EduDegree" client="EduDegree" type="C" required="true"  default="20"    desc=" 最高学位"/>  
					<field name="Staff" client="Staff" type="C" required="true"  default="20"    desc=" 是否本行员工"/>  
					<field name="IsStockHodler" client="IsStockHodler" type="C" required="true"  default="20"    desc="是否我行股东 "/>  
					<field name="IsRelative" client="IsRelative" type="C" required="true"  default="20"    desc=" 是否我行关联方"/>  
					<field name="Population" client="Population" type="C" required="true"  default="20"    desc=" 家庭人数"/>  
					<field name="Marriage" client="Marriage" type="C" required="true"  default="20"    desc=" 婚姻状况"/>  
					<field name="isOtherArea" client="isOtherArea" type="C" required="true"  default="20"    desc=" 是否异地"/>
					<field name="MobileTelephone" client="MobileTelephone" type="C" required="true"  default="20"    desc=" 手机号码"/>  
					<field name="FamilyStatus" client="FamilyStatus" type="C" required="true"  default="20"    desc=" 居住状况"/>  
					<field name="HaveWork" client="HaveWork" type="C" required="true"  default="20"    desc=" 有无单位"/>  
					<field name="WorkCorp" client="WorkCorp" type="C" required="true"  default="20"    desc="单位名称 "/>  
					<field name="HeadShip" client="HeadShip" type="C" required="true"  default="20"    desc=" 职务"/>  
					<field name="InComeSource" client="InComeSource" type="C" required="true"  default="20"    desc=" 收入来源"/>  
			</field>
		</rcv>
</trans>
