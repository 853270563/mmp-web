		<field name="signApprMark" length="450" type="C" desc="批复备注" />
		<field name="decaRes1" length="1" type="C" desc="预留1" />
		<field name="decaRes3" length="30" type="C" desc="预留3" />
        <field name="apprRetnInfo" type="C" desc="审核未通过原因" />	
		
