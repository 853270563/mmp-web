<?xml version="1.0" encoding="utf-8"?>
<trans name="yxPicInfo" desc="影像系统">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000100003" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="8160097" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
  				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field name="serial_no" client="serialNo" type="C"  required="true"   /> 
				<field name="user_id" client="userId" type="C" required="true"  default=""    desc="用户名" />  
		<field-extend name="page_control" type="S" xpath="Service_Body/page_control" desc="分页">
			<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="分页标非零0分页!0不分页" />
				<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="1"    desc="分页角码" />
				<field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="10"    desc="分页大小" />
			</field-extend>  
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<field name="status"     required="true" length="19"  xpath="Service_Header/service_response/status"   type="C" default="" desc=""/>
			<field name="code"     required="true" length="19"  xpath="Service_Header/service_response/code"   type="C" default="" desc=""/>
			<field name="desc"     required="true" length="19"  xpath="Service_Header/service_response/desc"   type="C" default="" desc=""/>
			<field name="total"     required="true" length="10"   xpath="Service_Body/response/total"   type="C" default="01" desc="总数"/>
			<!-- desc 格式说明    业务品种，字段含义/业务品种，字段含义-->
   			<field name="item"  xpath="Service_Body/response/item"  type="E"  desc="解析响应报文体">
					<field name="CustomerID" client="CustomerID" type="C" required="true"  default="20"    desc="客户编号" />  
					<field name="RelativeID" client="RelativeID" type="C" required="true"  default="20"    desc="关联客户编号" />  
					<field name="CustomerName" client="CustomerName" type="C" required="true"  default="20"    desc="姓名" />  
					<field name="CertType" client="CertType" type="C" required="true"  default="20"    desc="证件类型" />  
					<field name="CertID" client="CertID" type="C"  default="20"    desc="证件号码" />  
					<field name="CustomerType" client="CustomerType" type="C"   default="20"    desc="客户类型" />  
					<field name="RelationName" client="RelationName" type="C" required="true"  default="20"    desc="与该客户关系" />  
					<field name="Telephone" client="Telephone" type="C" required="true"  default="20"    desc="联系电话" />  
					<field name="Describe" client="Describe" type="C" required="true"  default="20"    desc="  工作单位" />  
					<field name="UserName" client="UserName" type="C" required="true"  default="20"    desc="登记人" />  
					<field name="OrgName" client="OrgName" type="C" required="true"  default="20"    desc="登记机构" />
					<field name="InputDate" client="InputDate" type="C" required="true"  default="20"    desc="登记日期" />
					<field name="UpdateDate" client="UpdateDate" type="C" required="true"  default="20"    desc="更新日期" />
			</field>
		</rcv>
</trans>