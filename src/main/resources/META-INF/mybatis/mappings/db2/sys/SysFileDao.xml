<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.yitong.ares.fileservice.dao.SysFileDao">
	
	<!-- 根据SESSIONID查找SKEY-->
    <select id="getSkeyBySessionId" parameterType="map" resultType="map">
       select KEY as "skey" from ARES_SESSION where SESSION_ID = #{sessionId}
    </select>	
</mapper>
