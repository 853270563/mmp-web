<?xml version="1.0" encoding="utf-8"?>
<trans name="test" desc="柜员签到">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080000100002" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="0018" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
			<!--分页-->
			<field-extend name="page_control" type="E" xpath="Service_Body/page_control" desc="分页">
				<field name="control_flag" client="control_flag" type="C" required="true"  default="1" desc="分页标记0分页!0不分页" />
				<field name="pageindex" client="PAGE_NO" type="C" required="true"  default="12"    desc="分页角码" />
				<field name="pagesize" client="PAGE_SIZE" type="C" required="true"  default="100"    desc="分页大小" />
			</field-extend>
				<!--请求体 -->
			<field-request name="request" type="E" xpath="Service_Body/request" desc="请求体">
					    
      				   <field  name="user_id" required="true"  default="2201" />
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<head>
   		    	<field-response name="service_response"  xpath="Service_Header/service_response" type="D" desc="解析响应报文头">
   		        	<field name="status" client="status" type="C" length="19" required="true"  	desc="交易状态"  />
					<field name="code"	client="code"   type="C" length="14"  required="true"  	desc="提供方响应码"   />
					<field name="desc" 	client="desc" 	 type="C" length="200" required="true"  desc="提供方响应描述"  />
   		   	 	</field-response>
   			</head>
   			<body>
   			     <field-response name="response"  xpath="Service_Body/response"  type="D"  desc="解析响应报文体">
					<field name="CM-OPN-RANG" client="CM_OPN_RANG" type="C" length="1"   desc="作业等级:柜员级别：A/B/1等等" />
					<field name="CM-EXCS-TX-SECN" client="CM_EXCS_TX_SECN" type="C" length="192"  desc="可执行交易组:192位岗位组" />
				</field-response>
			</body>
		</rcv>
</trans>
