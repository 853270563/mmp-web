<?xml version="1.0" encoding="utf-8"?>
<trans name="queryGstInfo" desc="查询手势密码">
	<!-- 请求报文 -->
	<snd>
		<field name="USER_ID" length="50" type="VARCHAR2" required="true" desc="用户ID" />
	</snd>
	<!-- 响应报文 -->
	<rcv>
		<!--公共字段-->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		<field name="GUESTER_FLAG" length="2" type="C" desc="0-不存在，1-存在" />
		<field name="GUESTER_PWD" length="100" type="C" desc="手势密码" />
	</rcv>
</trans>
