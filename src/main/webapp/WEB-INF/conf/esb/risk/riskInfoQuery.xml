<?xml version="1.0" encoding="utf-8"?>
<trans name="riskInfoQuery" desc="预警查询">
	<!--请求报文 -->
	<snd>
		<!--报文头 -->
		<head>
			<field name="service_id"     required="true" length="19"  xpath="Service_Header/service_id"   type="C" default="00080001000002" desc="ESB 服务码"/>
			<field name="requester_id" required="true" length="4"   xpath="Service_Header/requester_id" type="C" default="0018" desc="请求方系统代码"/>
			<field name="branch_id"      required="true" length="9"   xpath="Service_Header/branch_id"    type="C" default="802777777" desc="原始请求方机构代号 "/>
			<field name="channel_id"    required="true" length="2"   xpath="Service_Header/channel_id"   type="C" default="39" desc="服务渠道代号"/>
			<field name="version_id"     required="true" length="2"   xpath="Service_Header/version_id"   type="C" default="01" desc="版本号"/>
		</head>
		<!--报文体 -->
		<body>
				<!--请求体 -->
			<field-request name="request" type="S" xpath="Service_Body/request" desc="请求体">
				<field name="CustomerName" client="CustomerName" type="C"  desc="客户姓名" />
				<field name="CertID" client="CertID" type="C"  desc="证件号码" />
				<field name="SignalType" client="SignalType" type="C"   desc="01 一般预警 ，02 重大预警" />  
				<field name="BelongManDepart" client="BelongManDepart" type="C"  desc="0310 个人业务部 ，0110 公司业务部 ，0120 小企业贷款部" /> 
				<field name="UserID" client="UserID" type="C"   default=""    desc="用户编号" /> 
			</field-request>
		</body>
	</snd>
	<!-- 响应报文 -->
		<rcv>
			<head>
   		    	<field-response name="service_response"  xpath="Service_Header/service_response" type="S" desc="解析响应报文头">
   		 			<field name="status" client="status" type="C" length="19"  	desc="交易状态"  />
					<field name="code"	client="code"   type="C" length="14"   required="true" 	desc="提供方响应码"   />
					<field name="desc" 	client="desc" 	 type="C" length="200" required="true"  desc="提供方响应描述"  />
   		   	 	</field-response>
   			</head>
   			<body name="body">
   			    <field-response name="response"  xpath="Service_Body/response"  type="S"  desc="解析响应报文体">
					<field-list name="list"   type="E"  >
						<field name="SerialNo" client="SerialNo" type="C"   desc="预警信息流水号" />  
						<field name="CustomerName" client="CustomerName" type="C"     desc="客户名称" />  
						<field name="CertName" client="CertName" type="C"  desc="证件名称" />  
						<field name="CertID" client="CertID" type="C" desc="证件号码" />
						<field name="MessageContent" client="MessageContent" type="C"  desc="预警信号" />  
						<field name="SignalType" client="SignalType" type="C"   desc="预警类型" />
						<field name="BelongManDepart" client="BelongManDepart" type="C"   desc="归属管理部门" /> 
						<field name="InputUserName" client="InputUserName" type="C"    desc="登记人" /> 
						<field name="InputOrgName" client="InputOrgName" type="C"    desc="登记机构" /> 
						<field name="InputDate" client="InputDate" type="C"   desc="登记时间" /> 
						<field name="UpdateDate" client="UpdateDate" type="C"   desc="更新时间" /> 
					</field-list>
				</field-response>
			</body>
		</rcv>
</trans>