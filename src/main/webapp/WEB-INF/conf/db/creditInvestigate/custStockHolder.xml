<?xml version="1.0" encoding="utf-8"?>
<trans name="custStockHolder" ibatisStatement="" desc="客户股东信息">
	<!-- 请求报文，可进行必需项检查，设置缺省值，以及进行字段转换-->
	<snd>
		<!-- 客户股东信息  -->
		 <field  name="PAGE_NO" length="30" type="C" desc="第几页"  />
		<field  name="PAGE_SIZE" length="30" type="C" desc="每一页大小"  /> 
		<field  name="customerId" length="10" type="C" desc="客户名称" />
	</snd>
	<!-- 响应报文，可过滤响应信息，并对数据字典进行解析-->
	<rcv>
		<!--公共字段 -->
		<field name="STATUS" length="6" type="C" desc="返回码" />
		<field name="MSG" length="100" type="C" desc="返回消息" />
		 <!-- 代办记录 -->
        <field-list name="item" type="E">
            		<field name="relativeID" client="relativeID" type="C" required="true"  default="20"    desc="股东客户ID" />  
					<field name="customerName" client="customerName" type="C" required="true"  default="20"    desc="股东姓名" />  
					<field name="certType" client="certType" type="C" required="true"  default="20"    desc="客户证件类型" />  
					<field name="certID" client="certID" type="C" required="true"  default="20"    desc="证件号码" />  
					<field name="entType" client="entType" type="C" required="true"  default="20"    desc="股东类型" />  
					<field name="countryCodeName" client="countryCodeName" type="C"  default="20"    desc="国籍" />  
					<field name="isRealControl" client="isRealControl" type="C" required="true"  default="20"    desc="是否实际控制人" />  
					<field name="registerCode" client="registerCode" type="C" required="true"  default="20"    desc="登记注册代码" />  
					<field name="loanCardNo" client="loanCardNo" type="C" required="true"  default="20"    desc="股东贷款卡编号" />  
					<field name="fictitiousPerson" client="fictitiousPerson" type="C" required="true"  default="20"    desc="法人代表名称" />  
					<field name="relationShip" client="relationShip" type="C" required="true"  default="20"    desc="出资方式" />  
					<field name="currencyType" client="currencyType" type="C"  default="20"    desc="出资币种" />  
					<field name="oughtSum" client="oughtSum" type="C" required="true"  default="20"    desc="应出资金额" />  
					<field name="investmentSum" client="investmentSum" type="C" required="true"  default="20"    desc="实际投资金额" />  
					<field name="investmentProp" client="investmentProp" type="C" required="true"  default="20"    desc="出资比例" />  
					<field name="investDate" client="investDate" type="C" required="true"  default="20"    desc="投资日期" />  
					<field name="inputUserName" client="inputUserName" type="C"   default="20"    desc="登记人" />  
					<field name="inputOrgName" client="inputOrgName" type="C" required="true"  default="20"    desc="登记机构" />  
					<field name="inputDate" client="inputDate" type="C" required="true"  default="20"    desc="登记日期" /> 
					<field name="updateDate" client="updateDate" type="C" required="true"  default="20"    desc="更新日期" />   
        </field-list>
	</rcv>
</trans>
	